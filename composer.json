{
    "name": "iicn-dev/notification",
    "description": "A short description of what your package does",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "IICN\\Notification\\": "src/"
        },
        "classmap": [
            "database/factories/",
            "database/seeders/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
              "IICN\\Notification\\NotificationServiceProvider"
            ],
            "aliases": {
                "NotificationSender": "IICN\\Notification\\NotificationSender"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.5",
        "orchestra/testbench": "^8.20.0"
    },
    "autoload-dev": {
        "psr-4": {
            "IICN\\Notification\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true
}
